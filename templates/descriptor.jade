if item.id
  section.descriptor(id=item.id)
    .panel.panel-info
      .panel-heading
        h1.panel-title Descriptor: #{item.id}
      .panel-body
        +element_attributes(item)
        +element_content(item)
else
  a.btn.btn-primary(href=item.attributes.getValue('href'))= item.attributes.getValue('href')
